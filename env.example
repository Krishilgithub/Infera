# Vercel Analytics
VERCEL_ANALYTICS_ID=your_analytics_id_here

# Application Environment
NODE_ENV=production

# Site URL
SITE_URL=https://your-domain.com

# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=your_supabase_project_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key

# Realtime signaling (Socket.IO)
NEXT_PUBLIC_SOCKET_URL=http://localhost:4001
SOCKET_PORT=4001
SOCKET_CORS_ORIGIN=http://localhost:3000

# Email (Nodemailer)
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=your_smtp_user@example.com
SMTP_PASS=your_smtp_password_or_app_password
SMTP_FROM=Infera Meetings <no-reply@example.com>

# Google OAuth Configuration (for Supabase)
# Get these from Google Cloud Console and configure in Supabase Dashboard
# GOOGLE_CLIENT_ID=your_google_client_id
# GOOGLE_CLIENT_SECRET=your_google_client_secret

# Custom Environment Variables
CUSTOM_KEY=your_custom_key_here

# API Keys (if needed)
# API_KEY=your_api_key_here
